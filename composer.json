{
    "name": "qlimix/http-diactoros",
    "type": "library",
    "description": "Http helper interfaces implemented with diactoros and httphandlerrunner",
    "license": "MIT",
    "keywords": [
        "http"
    ],
    "authors": [
        {
            "name": "frank",
            "email": "frank@qlimix.net"
        }
    ],
    "require": {
        "php": ">=7.2",
        "psr/http-server-handler": "^1.0",
        "qlimix/http": "^1.0",
        "zendframework/zend-diactoros": "^2.1",
        "zendframework/zend-httphandlerrunner": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Qlimix\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qlimix\\Tests\\Http\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "qlimix/code-standard": "^1.0"
    }
}
